# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPods-Demo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPods-Demo
  
  #not specifying pod version will autodownload this
  pod 'Alamofire'
  
  #is a autolayout third party library
  pod 'SnapKit'
  
  #is a photo processing library
  pod 'Kingfisher'

end
